{
  "xds_servers": [
% for i, server in enumerate(servers):
    {
      "server_uri": "${server}",
      "channel_creds": [
        {
          "type": "insecure"
        }
      ],
      "server_features": [
        "xds_v3"
      ]
    }${'' if loop.last else ','}
% endfor
  ],
  "authorities": {
% for i, (name, server) in enumerate((authorities).items()):
    "${name}": {
      "xds_servers": [
        {
          "server_uri": "${server}",
          "channel_creds": [
            {
              "type": "insecure"
            }
          ],
          "server_features": [
            "xds_v3"
          ]
        }
      ]
    }${'' if loop.last else ','}
% endfor
  },
% if server_template:
  "server_listener_resource_name_template": "${server_template}",
% endif
  "node": {
    "id": "test-id",
    "cluster": "cluster",
    "locality": {
      "zone": "us-west4-b"
    }
  }
}
